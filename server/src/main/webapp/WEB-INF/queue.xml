<queue-entries>
    <queue>
        <name>optimized-queue</name>
        <rate>10/s</rate>
        <max-concurrent-requests>6</max-concurrent-requests>
        <retry-parameters>
            <task-retry-limit>1</task-retry-limit>
            <task-age-limit>1d</task-age-limit>
            <min-backoff-seconds>60</min-backoff-seconds>
        </retry-parameters>
    </queue>
</queue-entries>